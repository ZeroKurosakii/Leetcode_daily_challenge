class Solution {
public:
    bool search(vector<int>& nums, int target) {
        //sort(nums.begin() , nums.end());
        int tmp = count(nums.begin() , nums.end() , target);
        if(tmp >= 1){
            return true;
        }
        return false;
    }
};
